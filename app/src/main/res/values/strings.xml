    <string name="qst1">What is endometriosis?</string>
    <string name="qst1_opt1">A condition that affects the lungs</string>
    <string name="qst1_opt2">A disorder that affects the liver</string>
    <string name="qst1_opt3">A condition in which tissue similar to the lining of the uterus grows outside the uterus</string>
